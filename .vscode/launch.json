{
    "configurations": [
        {
            "name": "Install",
            "command": "npm install",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Start",
            "command": "npx strapi develop",
            "cwd": "${workspaceFolder}/hfmd-strapi",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Build",
            "command": "npx strapi build",
            "cwd": "${workspaceFolder}/hfmd-strapi",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Start (EB Docker Build)",
            "command": "./launch-scripts/run_strapi_eb_docker.sh",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Deploy (AWS EB)",
            "command": "./launch-scripts/aws_eb_deploy.sh",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "SSH (AWS EB)",
            "command": "./launch-scripts/aws_eb_ssh.sh",
            "request": "launch",
            "type": "node-terminal"
        },
    ]
}